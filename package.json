{
  "name": "match-report",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "npm run migrate:test && dotenv -e .env.test jest -- --runInBand",
    "test:cov": "npm run migrate:test && dotenv -e .env.test jest -- --runInBand --coverage",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "studio:test": "dotenv -e .env.test -- npx prisma studio",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "db:push": "dotenv -e .env.development -- npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/node": "18.16.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "chance": "^1.1.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "lodash": "^4.17.21",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prisma": "^4.13.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  }
